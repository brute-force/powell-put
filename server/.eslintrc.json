{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "max-len": [
        2,
        {
        "code": 120
        }
    ]
  }
}
